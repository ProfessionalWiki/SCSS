{
	"name": "mediawiki/scss",
	"type": "mediawiki-extension",
	"description": "Use SCSS files for MediaWiki styling",
	"keywords": [
		"wiki",
		"MediaWiki",
		"extension",
		"SCSS",
		"Sass"
	],
	"homepage": "https://www.mediawiki.org/wiki/Extension:SCSS",
	"readme": "README.md",
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Stephan Gambke",
			"email": "s7eph4n@protonmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/cmln/scss/issues",
		"forum": "https://www.mediawiki.org/wiki/Extension_talk:SCSS",
		"wiki": "https://www.mediawiki.org/wiki/Extension:SCSS",
		"irc": "irc://irc.freenode.org/mediawiki",
		"source": "https://github.com/cmln/scss",
		"docs": "https://github.com/cmln/scss/tree/latest/docs",
		"rss": "https://github.com/cmln/scss/releases.atom"
	},
	"require": {
		"php": ">=5.6",
		"composer/installers": "^1.0.1",
		"mediawiki/mediawiki": ">=1.27.0",
		"leafo/scssphp": "~0.7.5"
	},
	"autoload": {
		"psr-4": {
			"SCSS\\": "src/",
			"SCSS\\Tests\\": "tests/phpunit/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
